{
  "name": "bavarian/uploader",
  "type": "library",
  "description": "Uploader management for PHP applications.",
  "keywords": [
    "upload"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jorge Vahldick",
      "email": "jorge.dev@outlook.com"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "symfony/http-foundation": "~2.3|~3.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.4"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Bavarian\\Component\\Uploader\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bavarian\\Component\\Uploader\\spec\\": "spec/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
